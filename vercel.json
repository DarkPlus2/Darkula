{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/lookup.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/lookup",
      "dest": "/api/lookup"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
